CONFIG_RUST=y

config TMP11X_RS
    bool "TMP116, TMP117 and TMP119 Temperature Sensors Rust"
    default y
    depends on DT_HAS_TI_TMP11X_RS_ENABLED
    select I2C
    help
      Enable driver for TMP116, TMP117 and TMP119 temperature sensors.

if TMP11X_RS

module = TMP11X_RS
thread_priority = 10
thread_stack_size = 1024
source "drivers/sensor/Kconfig.trigger_template"

endif
